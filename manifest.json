{
  "manifest_version": 2,
  "name": "GeoJSON Viewer",
  "description": "This extension displays a GeoJSON response as a map.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Show GeoJSON"
  },
  "permissions": [
    "activeTab"
  ]
}
